<% switch(article.type ? article.type : "text") { 
    case "fieldset": %>
        <%- include content/fieldset.ejs %>
    <% break; %>
    <% default: %>
        <% if (content.type === "grid") { %>
            <%- include content/article_grid.ejs %>
        <% } else { %>
            <%- include content/article.ejs %>
        <% } %>
    <% break; %>
<% } %>